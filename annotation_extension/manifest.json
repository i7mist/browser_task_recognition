{
  "manifest_version": 2,
  "name": "browser task annotating helper",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Toggle replay mode"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "alarms"
  ]
}
